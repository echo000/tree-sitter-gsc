{
  "name": "tree-sitter-gsc",
  "version": "0.1.0",
  "description": "GSC (Game Script Code) grammar for tree-sitter - Call of Duty: Black Ops III",
  "main": "bindings/node",
  "types": "bindings/node/index.d.ts",
  "keywords": [
    "tree-sitter",
    "parser",
    "gsc",
    "csc",
    "call-of-duty",
    "black-ops-3"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "author": "GSCode Contributors",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Blakintosh/gscode"
  },
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "prebuildify": "^6.0.0",
    "tree-sitter-cli": "^0.22.6"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip",
    "build": "tree-sitter build --wasm",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse"
  },
  "tree-sitter": [
    {
      "scope": "source.gsc",
      "file-types": [
        "gsc",
        "csc",
        "gsh"
      ],
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm",
      "locals": "queries/locals.scm"
    }
  ]
}
